extends ../../public/jade/init
block init_html
  +init_html()
block panelFun
  case data.name
    when "left"
      .content-block-title 左侧面板 #{data.name}
        .content-block
          a(href="#").close-panel 关闭我
          br
          a(href="#", data-panel="right").open-panel.col-100 右侧面板
        .content-block-title 功能表
        +list-blockFun({
            data:[
              {name: "表单", url: "forms.html"},
              {name: "List View", url: "list-view.html"},
              {name: "Media Lists", url: "media-lists.html"},
              {name: "Modals", url: "modals.html"},
              {name: "Navbars And Toolbars", url: "bars.html"},
              {name: "Popover", url: "popover.html"},
              {name: "Side Panels", url: "panels.html"},
              {name: "Swipe To Delete", url: "swipe-delete.html"},
              {name: "Swiper Slider", url: "swiper.html"},
              {name: "Tabs", url: "tabs.html"},
            ],
            bool:true
        })
    when "right"
      .view.view-right
        .navbar
          .navbar-inner
            .center.sliding 右侧面板
        .pages.navbar-through
          .page(data-page="panel-right1")
            .page-content
              .content-block
                a(href="#").close-panel 关闭我
              +list-blockFun({
                  data: [
                      {name: "右面板页面2", url: "panel-right2.html"},
                      {name: "右面板页面3", url: "panel-right3.html"},
                  ]
              })
block viewFun
  +navs({
      left:{
        bool:false,
        txt:"Sdfsd"
      }
  })
  .toolbar
    .toolbar-inner
      a(href="#").link Dummy Link
      a(href="#", data-popover=".popover-menu").open-popover.link Menu
  .pages.navbar-through.toolbar-through
    .page(data-page="index")
      .page-content
        .content-block-title 欢迎来到FRAMEWORK7
        .content-block
          a(href="about.html").button 关于FRAMEWORK7
        .content-block-title 功能列表
        +list-blockFun({
            data: [
                {name: "折叠地图", url: "accordion.html"},
                {name: "补全/自动补全", url: "autocomplete.html"},
                {name: "日历 / 选择日历", url: "calendar.html"},
