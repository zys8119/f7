                {name: "时间轴", url: "timeline.html"},
                {name: "列表数据搜索", url: "virtual-list.html"},
            ]
        })
        +list-blockFun({
            data: [
                {name: "动画", url: "animation.html"},
                {name: "主题颜色", url: "color-themes.html"},
                {name: "动态生成", url: "#",className:" ks-generate-page "},
            ]
        })
        +list-blockFun({
            data: [
                {name: "过度和效果", url: "transitions.html"},
                {name: "核心特征", url: "core-features.html"},
            ]
        })
